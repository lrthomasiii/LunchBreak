<table>
	<tr>
		<td>Name</td>
		<td>Email</td>
		<td>Password</td>
	</tr>
	<% User.all.each do |user| %>
	<tr>
		<td><%= user.name %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= link_to "Show User", user_path(user.id) %></td>
		<td><%= link_to "Edit User", edit_user_path(user.id) %></td>
		<td><%= button_to "Destroy User", user, confirm: "Are you sure you want to delete this user?", method: :delete %></td>
	</tr>
	<% end %>
</table>
<%= link_to "New User", new_user_path %>
<%= link_to "Back to Home", home_index_path %>
