<table>
	<tr>
		<td>Title</td>
		<td>Date</td>
		<td>Time</td>
		<td>Description</td>
		<td>User</td>
	</tr>
	<% Event.all.each do |event| %>
	<tr>
		<td><%= event.title %></td>
		<td><%= event.date %></td>
		<td><%= event.time %></td>
		<td><%= event.description %></td>
		<td><%= event.user.name %></td>
	</tr>
	<% end %>
</table>
<%= link_to "Back to Home", home_index_path %>